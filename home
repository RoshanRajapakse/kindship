<html>
<head>
  <style>
    body {
      background-color: rgba(255,243,238,255);
    }
  </style>
</head>
<body>
  <form>
    <label for="input1">Input 1:</label><br>
    <input type="text" id="input1" name="input1"><br>
    <label for="input2">Input 2:</label><br>
    <input type="text" id="input2" name="input2"><br>
    <label for="input3">Input 3:</label><br>
    <input type="text" id="input3" name="input3"><br>
    <label for="input4">Input 4:</label><br>
    <input type="text" id="input4" name="input4"><br><br>
    <input type="button" value="Submit" onclick="calculate()">
  </form> 

  <div id="output">Output:</div>

  <script>
    function calculate() {
      // Get the values of the input fields
      var input1 = document.getElementById("input1").value;
      var input2 = document.getElementById("input2").value;
      var input3 = document.getElementById("input3").value;
      var input4 = document.getElementById("input4").value;

      // Perform some calculation on the input values
      var result = input1 + input2 + input3 + input4;

      // Display the result in the output field
      document.getElementById("output").innerHTML = "Output: " + result;
    }
  </script>
</body>
</html>
